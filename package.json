{
  "name": "teleport-generator-vue",
  "version": "0.0.1",
  "description": "Vue code generator for teleportHQ",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "tsc -w",
    "build": "npm run clean && tsc",
    "link-dev": "npm link ../teleport-lib-js",
    "clean": "rm -rf dist",
    "lint": "tslint -p ./",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "npm run test -- --coverage --no-cache",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/reapp/teleport-generator-vue.git"
  },
  "author": "teleportHQ",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/reapp/teleport-generator-vue/issues"
  },
  "homepage": "https://gitlab.com/reapp/teleport-generator-vue#README",
  "devDependencies": {
    "@types/jest": "^23.1.3",
    "codecov": "^3.0.2",
    "husky": "^0.14.3",
    "jest": "^23.2.0",
    "pretty-quick": "^1.6.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.9.2",
    "prettier": "^1.13.6"
  },
  "dependencies": {
    "@types/node": "^9.6.0",
    "lodash": "^4.17.5",
    "vue-formatter": "0.0.7",
    "@teleporthq/teleport-lib-js": "0.0.3"
  }
}
