{
  "content": {
    "name": "TestComponent",
    "type": "View",
    "source": "teleport-elements-core",
    "style": ["TestComponent"],
    "children": [{
      "source": "teleport-elements-core",
      "type": "Image",
      "name": "author",
      "style": ["author"],
      "props": {
        "src": "$props.src",
        "alt": "$props.name",
        "children": [{
          "type": "TestComponent1",
          "source": "components"
        }]
      }
    }, {
      "type": "TestComponent1",
      "source": "components"
    }, {
      "type": "Text",
      "source": "teleport-elements-core",
      "name": "Text",
      "props": {
        "height": "100%"
      },
      "style": ["Text"]
    }, {
      "source": "teleport-elements-core",
      "type": "View",
      "name": "Author",
      "style": ["Author"],
      "children": [{
        "source": "teleport-elements-core",
        "type": "Text",
        "name": "Name",
        "children": "$props.name"
      }, {
        "source": "teleport-elements-core",
        "type": "Link",
        "name": "Role",
        "children": [{
          "name": "link",
          "source": "teleport-elements-core",
          "type": "A",
          "children": "$props.twitterHandle",
          "style": ["link"]
        }],
        "props": {
          "href": "$props.twitterLink"
        }
      }]
    }]
  },
  "dependencies": {
    "./TestComponent1": [{
      "defaultImport": true,
      "type": "TestComponent1"
    }]
  },
  "styles": {
    "TestComponent": {
      "display": "flex"
    },
    "author": {
      "height": "60px",
      "width": "60px",
      "borderRadius": "50%",
      "border": "5px solid #fff"
    },
    "Text": {
      "width": "100%",
      "height": "$props.height",
      "flexDirection": "row",
      "backgroundColor": "pink"
    },
    "Author": {
      "display": "flex",
      "flexDirection": "column",
      "justifyContent": "center",
      "marginLeft": "15px",
      "lineHeight": "1.2"
    },
    "link": {
      "&:hover": {
        "textDecoration": "underline"
      }
    }
  },
  "props": {

  }



}